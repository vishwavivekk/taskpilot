{
  "name": "@taskpilot/backend",
  "version": "0.1.0",
  "description": "TaskPilot Backend - NestJS API Server",
  "author": "TaskPilot Team",
  "private": true,
  "license": "SEE LICENSE IN https://github.com/TaskPilot/TaskPilot/blob/main/LICENSE.md",
  "scripts": {
    "build": "npm run prisma:generate && nest build",
    "build:dist": "cross-env BUILD_DIST=true npm run build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"test/**/*.ts\"",
    "format:all": "npm run format && npm run lint",
    "start": "nest start",
    "start:dev": "npm run prisma:generate && nest start --watch",
    "start:debug": "npm run prisma:generate && nest start --debug --watch",
    "start:prod": "npm run prisma:generate && node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint:check": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json --runInBand",
    "prisma:generate": "prisma generate",
    "prisma:studio": "prisma studio",
    "prisma:migrate:dev": "prisma migrate dev",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:reset": "prisma migrate reset",
    "prisma": "prisma",
    "seed": "ts-node -r tsconfig-paths/register src/seeder/seeder.command.ts seed",
    "seed:admin": "ts-node -r tsconfig-paths/register src/seeder/seeder.command.ts admin",
    "seed:clear": "ts-node -r tsconfig-paths/register src/seeder/seeder.command.ts clear",
    "seed:reset": "ts-node -r tsconfig-paths/register src/seeder/seeder.command.ts reset",
    "clean": "shx rm -rf node_modules dist out .next"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.858.0",
    "@aws-sdk/s3-request-presigner": "^3.858.0",
    "@nestjs/bull": "^11.0.3",
    "@nestjs/bullmq": "^11.0.3",
    "@nestjs/cli": "^11.0.10",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.1",
    "@nestjs/core": "^11.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.3",
    "@nestjs/platform-socket.io": "^11.1.3",
    "@nestjs/schedule": "^6.0.1",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/websockets": "^11.1.3",
    "@prisma/client": "^6.6.0",
    "@types/mime-types": "^3.0.1",
    "@types/multer": "^2.0.0",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/sanitize-html": "^2.16.0",
    "bcrypt": "^5.1.1",
    "better-queue": "^3.8.12",
    "bull": "^4.16.5",
    "bullmq": "^5.61.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "helmet": "^8.1.0",
    "html-entities": "^2.6.0",
    "imapflow": "^1.1.1",
    "ioredis": "^5.6.1",
    "mailparser": "^3.7.4",
    "mime": "^4.0.7",
    "mime-types": "^3.0.1",
    "multer": "^2.0.2",
    "multer-s3": "^3.0.1",
    "nodemailer": "^7.0.6",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "prisma": "^6.19.0",
    "redis": "^5.5.6",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "sanitize-html": "^2.17.0",
    "slugify": "^1.6.6",
    "socket.io": "^4.8.1",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@types/bcrypt": "^5.0.2",
    "@types/better-queue": "^3.8.6",
    "@types/bull": "^3.15.9",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/marked": "^5.0.2",
    "@types/node": "^22.10.7",
    "@types/nodemailer": "^6.4.20",
    "@types/supertest": "^6.0.2",
    "cross-env": "^7.0.3",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "ignore-loader": "^0.1.2",
    "jest": "^29.7.0",
    "marked": "^17.0.1",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0",
    "webpack": "^5.100.2",
    "webpack-cli": "^6.0.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/$1"
    }
  }
}